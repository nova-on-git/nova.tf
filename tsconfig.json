{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "paths": {
          "*": ["./types/*"]
      }
  }
}
